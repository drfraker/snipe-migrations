{
    "name": "drfraker/snipe-migrations",
    "description": "Blazing fast database migrations for Laravel tests.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Fraker",
            "email": "drfraker@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^10.0",
        "orchestra/testbench": "^3.7|^8.0",
        "php-mock/php-mock-mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Drfraker\\SnipeMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drfraker\\SnipeMigrations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drfraker\\SnipeMigrations\\SnipeMigrationsServiceProvider"
            ]
        }
    }
}
